{
  "name": "toolpages/toolpages",
  "description": "toolpages â€“ self-hosted dashboard (CodeIgniter 4)",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "codeigniter4/framework": "^4.6",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-intl": "*",
    "ext-ldap": "*",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "fakerphp/faker": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "test": [
      "@php vendor/bin/phpunit"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.3.0"
    }
  }
}
